<?xml version="1.0" encoding="UTF-8" ?>

<configuration status="WARN">
    <Properties>
        <Property name="basePath">./logs</Property>
    </Properties>
    <Appenders>
        <console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36 -%msg%n"/>
        </console>
        <RollingFile name
    </Appenders>
</configuration>